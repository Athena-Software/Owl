{
	"name": "Owl - Password Access and Autofill for KeePass 2.x",
	"short_name": "Owl",
	"version": "2019.12.28",
	"manifest_version": 2,
	"minimum_chrome_version": "48",
	"description": "Readonly KeePass 2.x password database integration for Firefox",
	"icons": {
		"16": "/assets/icons/exported/owl_16x20.png",
		"48": "/assets/icons/exported/owl_48x70.png"
	},
	"background": {
		"scripts": [
			"dll/dll.library.js",
			"build/background.build.js"
		]
	},
	"browser_action": {
		"default_icon": "assets/icons/exported/owl_38x43.png",
		"default_popup": "popup.html",
    "default_title": "Owl"
	},
	"options_ui": {
		"page": "options.html",
		"chrome_style": false,
		"open_in_tab": true
	},
	"browser_specific_settings": {
	  "gecko": {
	    "id": "contact@soeren.codes",
	    "strict_min_version": "57.0"
	  }
	},
	"commands": {
		"_execute_browser_action": {
			"suggested_key": {
				"windows": "Ctrl+Shift+Space",
				"mac": "Command+Shift+Space",
				"chromeos": "Ctrl+Shift+Space",
				"linux": "Ctrl+Shift+Space",
				"default": "Ctrl+Shift+Space"
			}
		}
	},
	"static_data": {
		"dropbox" : {
			"client_id": "exkvwmhrrromtzh"
		},
		"onedrive" : {
			"client_id" : "352beab6-53e8-4187-9b93-c5915dd268fe"
		},
		"gdrive": {
			"client_id": "212480071870-s2opocg9q728787qph4eogsko25gsnt9.apps.googleusercontent.com"
		},
		"pcloud": {
			"client_id": "JL6WkJefyG7"
		}
	},
	"permissions": [
		"activeTab",
		"storage",
		"clipboardWrite",
		"identity",
		"alarms",
		"notifications",
		"https://*/*",
		"http://*/*",
		"file:///*/*"
	],
	"optional_permissions": [
		"https://*/*",
		"http://*/*"
	]
}
