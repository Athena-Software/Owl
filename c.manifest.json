		"chrome_style": false,
		"open_in_tab": true
	},
	"commands": {
		"_execute_browser_action": {
			"suggested_key": {
				"windows": "Ctrl+Shift+Space",
				"mac": "Command+Shift+Space",
				"chromeos": "Ctrl+Shift+Space",
				"linux": "Ctrl+Shift+Space",
				"default": "Ctrl+Shift+Space"
			}
		}
	},
	"permissions": [
		"activeTab",
		"storage",
		"clipboardWrite",
		"identity",
		"alarms",
